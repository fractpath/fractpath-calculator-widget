{
  "name": "fractpath-calculator-widget",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5000 --strictPort",
    "build": "tsc -p tsconfig.build.json && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "build:compute": "npm -w @fractpath/compute run build",
    "prebuild": "npm run build:compute"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "vitest": "^4.0.18",
    "@fractpath/compute": "workspace:*"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist-types/lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist-types/lib/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "workspaces": [
    "packages/*"
  ]
}
